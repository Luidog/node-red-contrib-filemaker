{
  "name": "node-red-contrib-filemaker",
  "version": "0.0.3",
  "description": "A node-red filemaker module.",
  "scripts": {
    "dev": "nodemon --exec \"node-red\" ./*.*",
    "test": "nyc _mocha --recursive  \"test/**/*_spec.js\" --timeout=30000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "generate-readme": "mos"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Luidog@github.com/Luidog/node-red-contrib-filemaker.git"
  },
  "keywords": [
    "node-red",
    "filemaker",
    "data",
    "api",
    "node"
  ],
  "author": "Lui de la Parra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Luidog/node-red-contrib-filemaker/issues"
  },
  "mos": {
    "plugins": [
      "execute"
    ]
  },
  "node-red": {
    "nodes": {
      "create-record": "create.js",
      "perform-find": "find.js",
      "trigger-script": "script.js",
      "client": "client.js"
    }
  },
  "homepage": "https://github.com/Luidog/node-red-contrib-filemaker#readme",
  "dependencies": {
    "fms-api-client": "^1.4.5",
    "marpat": "^1.18.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^4.11.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mos": "^1.3.1",
    "mos-plugin-dependencies": "^2.2.2",
    "mos-plugin-installation": "^1.2.2",
    "mos-plugin-license": "^1.2.2",
    "mos-plugin-execute": "^1.0.2",
    "node-red": "^0.19.3",
    "node-red-node-test-helper": "^0.1.8",
    "nyc": "^11.8.0",
    "prettier": "^1.8.2"
  }
}
